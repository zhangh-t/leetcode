class Solution {
public:
    bool isMatch(string s, string p) {
        int nStringLength = s.length();
        int nPatternLength = p.length();
        if (nStringLength == 0) return nPatternLength == 0;
        else if(nPatternLength == 0) {return false;}
        else
        {
        	 	int i = 0;
        	 	int nMax = nStringLength > nPatternLength ? nStringLength : nPatternLength;
        	 	bool bResult = true;
        	 	bool bCurResult = true;
        	 	char cAny = '';
        	 	while(i < nMax)
        	 	{
        	 		 bResult = bResult && bCurResult;
        	 	}
        	 	return bResult;     	 	
        }
    }
};